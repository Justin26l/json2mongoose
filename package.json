{
  "name": "json2mongoose",
  "version": "0.1.0",
  "description": "genarate mongoose ts files from jsonSchema",
  "author": "justin26l",
  "license": "MIT",
  "main": "./dist/index.js",
  "bin": {
    "json2mongoose": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "ts-node src/cli.ts ./jsonSchema ./output",
    "generate": "node dist/cli.js ./jsonSchema ./output",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Justin26l/json2mongoose.git"
  },
  "keywords": [
    "jsonschema",
    "mongoose"
  ],
  "dependencies": {
    "@types/mongoose": "^5.11.97",
    "json-schema-to-typescript": "^13.1.1"
  }
}
